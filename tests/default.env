# PRODUCT ENV
# APP
APP_TITLE=Заявки пользователей API
DEV_MODE=True
ORIGINS=["*"]

# Postgres
POSTGRES_USER=admin
POSTGRES_PASSWORD=123
POSTGRES_HOST=test_db
POSTGRES_PORT=5435
POSTGRES_DB=userrequest_db

# Kafka
KAFKA_PRODUCER_HOST=test_kafka
KAFKA_PRODUCER_PORT=9093

KAFKA_ADVERTISED_LISTENERS=PLAINTEXT://test_kafka:9093
KAFKA_LISTENERS=PLAINTEXT://0.0.0.0:9093
KAFKA_ZOOKEEPER_CONNECT=test_zookeeper:2182
KAFKA_BROKER_ID=1
KAFKA_LISTENER_SECURITY_PROTOCOL_MAP=PLAINTEXT:PLAINTEXT
KAFKA_LOG_DIRS=/var/lib/kafka/data

# Zookeeper
ZOOKEEPER_CLIENT_PORT=2182
ZOOKEEPER_TICK_TIME=2000
